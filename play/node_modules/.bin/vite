#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/zhangli/learning_code/Vue3-summary-zl/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/zhangli/learning_code/Vue3-summary-zl/node_modules/.pnpm/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../node_modules/.pnpm/vite@3.0.3/node_modules/vite/bin/vite.js" "$@"
else
  exec node  "$basedir/../../../node_modules/.pnpm/vite@3.0.3/node_modules/vite/bin/vite.js" "$@"
fi
